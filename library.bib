keywords = {Antennas,Array signal processing,Cell-free massive multiple-input multiple-output,MIMO communication,Multiplexing,NOMA,Unicast,Wireless networks,energy efficiency,first-order algorithm,layered-division multiplexing,non-orthogonal multicast and unicast transmission},
