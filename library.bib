keywords = {Beamforming,Internet-of-Things (IoT) communications,Large intelligent surface/antennas (LISA),Massive access,Symbiotic radio (SR)},
pages = {1--30},
title = {{Large Intelligent Surface/Antennas (LISA) assisted symbiotic radio for IoT communications}},
year = {2020}
}
