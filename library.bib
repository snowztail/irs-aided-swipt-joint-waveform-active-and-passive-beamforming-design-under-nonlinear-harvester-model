issn = {0018-9448},
journal = {IEEE Transactions on Information Theory},
number = {10},
pages = {6488--6508},
title = {{On Capacity-Achieving Distributions for Complex AWGN Channels Under Nonlinear Power Constraints and Their Applications to SWIPT}},
volume = {66},
year = {2020}
}
