keywords = {Array signal processing,Energy Harvesting,Energy harvesting,Erbium,Intelligent Reflecting Surface (IRS),Iterative methods,Large Intelligent Surface (LIS),MIMO,MIMO communication,Optimization,SWIPT,Wireless communication},
