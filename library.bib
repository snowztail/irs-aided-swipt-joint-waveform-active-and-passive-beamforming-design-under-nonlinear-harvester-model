keywords = {Antenna,CMOS rectifier,RF energy harvester,WSN,co-design,integration,sensitivity},
number = {3},
pages = {622--634},
publisher = {IEEE},
title = {{Co-design of a CMOS rectifier and small loop antenna for highly sensitive RF energy harvesters}},
volume = {49},
year = {2014}
}
